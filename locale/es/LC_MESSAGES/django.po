msgid "Django Signals"
msgstr "Señales de Django"

#: templates/tip.html:162
msgid ""
"Four signals: <code class=\"text-xs bg-base-200 px-1 "
"rounded\">payment_completed</code>, <code class=\"text-xs bg-base-200 px-1 "
"rounded\">payment_cancelled</code>, <code class=\"text-xs bg-base-200 px-1 "
"rounded\">payment_expired</code>, <code class=\"text-xs bg-base-200 px-1 "
"rounded\">refund_sent</code>."
msgstr ""
"Cuatro señales: <code class=\"text-xs bg-base-200 px-1 "
"rounded\">payment_completed</code>, <code class=\"text-xs bg-base-200 px-1 "
"rounded\">payment_cancelled</code>, <code class=\"text-xs bg-base-200 px-1 "
"rounded\">payment_expired</code>, <code class=\"text-xs bg-base-200 px-1 "
"rounded\">refund_sent</code>."

