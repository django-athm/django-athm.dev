#: templates/tip.html:240
msgid "Via Admin or manage.py"
msgstr "Mediante Admin o manage.py"

#: templates/tip.html:245
msgid "Done"
msgstr "Ya está"

#: templates/tip.html:246
msgid "Transactions sync automatically"
msgstr "Las transacciones se sincronizan automáticamente"

